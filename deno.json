{
  "tasks": {
    "dev": "deno run --allow-all --watch=./index.ts,./src,./packs,./lib index.ts",
    "clean": "deno run --allow-all index.ts --clean",
    "packs": "deno run --allow-all index.ts --packs",
    "build": "deno run --allow-all index.ts --build"
  },
  "imports": {
    "@kisu/api": "./lib/@kisu/api/index.ts",
    "@kisu/cli": "./lib/@kisu/cli/index.ts"
  },
  "lint": {
    "rules": {
      "exclude": ["no-import-prefix"]
    }
  },
  "exclude": [
    "./data/dist"
  ]
}
